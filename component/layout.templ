package component;

import "wikinow/ast"

templ Layout(node *ast.Node) {
	<!DOCTYPE html>
	<html lang="en">
		<head>
			<meta charset="UTF-8"/>
			<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
			<title>Document</title>
		</head>
		<body>
			@Parser(node)
		</body>
	</html>
}
